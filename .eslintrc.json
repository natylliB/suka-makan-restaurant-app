{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "class-methods-use-this": "off",
      "no-console": "off",
      "no-restricted-globals": "off",
      "import/no-extraneous-dependencies": "off"
    }
}
